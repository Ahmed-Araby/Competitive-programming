// the output of the given test cases is tricky and we don`t have to diplay such output cuz it is difficult to produce
#include <bits/stdc++.h>
using namespace std;
int main()
{
    int n , d;;
    cin>>n>>d;
    if(d==0 && n!=1)  // only case that we can`t construct the number that he want 
        cout<<"No solution"<< endl;
    else {cout<<d;for(int i=1; i<n; i++)cout<<0;}
    cout<< endl;
    return 0;
}

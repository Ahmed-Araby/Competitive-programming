#include <iostream>
using namespace std;
int main()
{
    int n , array[100000][2];
    cin>>n;
    for(int i=0; i<n; i++)
    for(int j=0; j<2; j++)
    cin>>array[i][j];
    for(int i=0; i<n; i++)
    {
        if(array[i][0]!=array[i][1])  // if the prize of the laptop is not equal to it`s quality 
        {
            cout<<"Happy Alex"<< endl;
            return 0;
        }
    }
    cout<<"Poor Alex"<< endl;
    return 0;
}

// I don`t know how does it have no timelimit exceed , it`s dependent on the complixety of the sqrt function I need to know how it`s implemented 
#include <iostream>
#include <cmath>
using namespace std;
int main()
{
    int n , x , pre=-1000000, q;
    cin>>n;
    for(int i=0; i<n; i++)
    {
        cin>>x;
        q=sqrt(x);
        q*q!=x&&x>pre?pre=x:pre=pre;
    }
    cout<<pre<< endl;
    return 0;
}

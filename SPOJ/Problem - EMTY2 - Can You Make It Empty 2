// without using Data Stracture 
#include <iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    for(int j=1; j<=t; j++)
    {
        string word;
        cin>>word;
        int c1=0 , c0=0;
        for(int i=0; i<word.size(); i++)
            {
                if(word[i]=='1')
                c1++;
                else
                    c0++;
                if(c0>2*c1)
                    break;
            }
            if(c0>2*c1 || 2*c1>c0)
                cout<<"Case "<<j<<": no"<< endl;
            else
                cout<<"Case "<<j<<": yes"<< endl;
    }
    return 0;
} 

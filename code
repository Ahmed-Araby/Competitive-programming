#include <iostream>
using namespace std;
int main()
{
    int n , counter=0;
    cin>>n;
    char array[120][120];
    for(int i=1; i<=n; i++)
    for(int j=1; j<=n; j++)
    cin>>array[i][j];
    for(int i=1; i<=n; i++)
    {
        for(int j=1; j<=n; j++)
        {
            // check for one cell
            if(array[i][j-1]=='o') // left of the cell
                counter++;
            if(array[i][j+1]=='o') // right of the cell
                counter++;
            if(array[i+1][j]=='o')  // down the cell
                counter++;
            if(array[i-1][j]=='o')  // above the cell
                counter++;
            if(counter%2==0 && counter!=0)
            {
                cout<<"YES"<< endl;
                return 0;  // end the program cuz there is one cell have even number of adjacent cells with 'o'
            }
            counter=0;
        }
    }
    cout<<"NO"<< endl;
    return 0;
}

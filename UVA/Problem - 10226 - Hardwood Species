// no that this input and output is little tricky  
//followed by blank line here mean that I enter it by hitting enter not by cout<< endl; and that is part of the input ^^^
//but print blank line mean  cou<< endl; and that is part of the format for the output 
// search for the getline and notice how it work 
#include <iostream>
#include <map>
#include <iomanip>
using namespace std;
int main()
{
    int n;
    cin>>n;
    string word;
    // theses thing to make the getline(cin , word> in the while loop go will without errors 
    getline(cin , word);  // that to take over the newline that is generated by hitting enter to enter the variable n
    getline(cin , word);  // that to take over the blank line that I will generate by hitting enter that is part of the input 
    for(int i=0; i<n; i++)
    {
        float counter=0;  // the total number of trees
        map<string , float> tree;
        while(getline(cin , word))
        {
            if(word.empty())  // the blank line between inputs of the consecutive test cases
                break;
            tree[word]++;
            counter++;
        }
        for(map<string , float>::iterator itr=tree.begin(); itr!=tree.end(); itr++)
            cout<<itr->first<<" "<<fixed<<setprecision(4)<<((itr->second/counter)*100)<< endl;
        if(i!=n-1)
            cout<< endl; // the blank line between the output of consecutive test cases
    }
    return 0;
}
